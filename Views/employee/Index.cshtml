@model List<Employee>

<h2> ALL Employee</h2>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Age</th>
      <th scope="col">Titel</th>
    </tr>
  </thead>
  <tbody>
    @foreach  (var item in Model)
     { <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Age</td>
        <td>@item.Titel</td>
       </tr>
     }
  </tbody>
</table>

